    <InstallExecuteSequence>
      <Custom Action="checkAVX" Before="InstallFinalize" ></Custom>
    </InstallExecuteSequence>
    
    <Media Id='1' Cabinet='DPlus.cab' EmbedCab='yes' />
    
        
    <PropertyRef Id="WIX_IS_NETFRAMEWORK_40_OR_LATER_INSTALLED"/>
